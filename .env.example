TIKHUB_KEYWORD=search_keyword
TIKHUB_CURSOR=0
TIKHUB_SORT_TYPE=0
TIKHUB_PUBLISH_TIME=0
TIKHUB_FILTER_DURATION=0
TIKHUB_CONTENT_TYPE=0
TIKHUB_SEARCH_ID=
TIKHUB_BACKTRACE=
TIKHUB_HOST=api.tikhub.io
TIKHUB_PATH=/api/v1/douyin/search/fetch_general_search_v3
